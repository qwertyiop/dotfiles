* {
  background-color: #1c1c1c;
  text-color: #e4e4e4;
  spacing: 0;
  width: 720px;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt, entry];
}

prompt {
  padding: 16px;
}

textbox {
  padding: 8px 16px;
}

entry {
  padding: 16px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  padding: 16px;
}

element selected {
  background-color: #5be3e3;
  text-color: #1c1c1c;
}
