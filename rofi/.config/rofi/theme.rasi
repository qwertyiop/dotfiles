* {
  background-color: transparent;
  text-color: #d8dee9;
  spacing: 0;
}

window {
  background-color: #1c1c1c;
  width: 720px;
  height: 400px;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
}

textbox {
  padding: 8px 16px;
}

entry {
  padding: 16px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  padding: 16px;
}

element .selected {
  background-color: #737680;
  text-color: #1c1c1c;
}
