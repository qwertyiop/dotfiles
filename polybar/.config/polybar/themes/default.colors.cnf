# Default color theme

[color]

# color.bg sets the default background color for the bar and modules 
# unless an alternative is specified. It also sets the backgound 
# for the suffixes and the prefixes which extend their background
bg = #00x000000

# color.bg_alt sets the default background for the modules and the
# foreground for the suffixes and the prefixes which extend their
# background
bg_alt = #000000

# default foreground color, used by modules
fg = #D7D7D7

# foreground color for window tiltle and application launchers
fg_alt = #000000

# color for warnings, here a shade of red
warning = #FF5F5F

# "normal state" color, used only be the battery module
# for format-charging-background 
normal = #5FFF5F
